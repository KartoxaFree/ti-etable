<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetable2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Timetable2.NoActionBar">
        <activity
            android:name=".syncingOnline"
            android:windowSoftInputMode="adjustPan"
            android:exported="false" />
        <activity android:name=".editor" />
        <activity
            android:name=".info"
            android:label="@string/title_activity_info"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/Theme.Timetable2.NoActionBar" />
        <activity
            android:name=".build"
            android:label="@string/title_activity_bild"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/Theme.Timetable2.NoActionBar" />
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_setings"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/Theme.Timetable2.NoActionBar" />
        <activity
            android:name=".fullTable"
            android:label="@string/title_activity_full_table"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/Theme.Timetable2.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>